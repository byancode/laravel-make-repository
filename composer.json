{
    "name": "byancode/laravel-make-repository",
    "description": "Simple repository pattern implementation for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "byancode",
            "email": "info@byancode.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Byancode\\Repository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Byancode\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
